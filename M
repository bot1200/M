    button.Position = UDim2.new(0.85, -50, 0.15, -50)
    button.Text = "Shoot"
    button.Font = Enum.Font.Gotham
    button.TextSize = 13
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.BackgroundColor3 = Color3.fromRGB(138, 43, 250)
